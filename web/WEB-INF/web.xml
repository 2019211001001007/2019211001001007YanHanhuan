<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
    <servlet-name>a</servlet-name><!--give any name -->
    <servlet-class>com.example.YanHanhuan2019211001001007.HelloWorldServlet</servlet-class><!--package +name of servlet class -->
    </servlet>
    <servlet-mapping>
    <servlet-name>a</servlet-name>
    <url-pattern>/hello</url-pattern><!--url must start with /-->
    </servlet-mapping>
    <servlet>
        <servlet-name>anyName</servlet-name><!--give any name -->
        <servlet-class>com.example.yanhanhuan.week2.exercise</servlet-class><!--package +name of servlet class -->
    </servlet>
    <servlet-mapping>
        <servlet-name>anyName</servlet-name>
        <url-pattern>/hapi</url-pattern><!--url must start with /-->
    </servlet-mapping>
</web-app>